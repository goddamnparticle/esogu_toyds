{
    "configurations": {
        "Launch": {
            "adapter": "vscode-cpptools",
            "filetypes": [
                "cpp",
                "c",
                "objc",
                "rust"
            ],
            "configuration": {
                "request": "launch",
                "program": "${fileDirname}/${fileBasenameNoExtension}",
                "args": [
                    "-p",
                    "1024",
                    "-d",
                    "/home/yorek/sample/data/ESOGU_ToyDS_A/",
                    "-c",
                    "10",
                    "-r",
                    "16"
                ],
                "cwd": "$workspaceRoot",
                "environment": [],
                "stopAtEntry": true,
                "externalConsole": true,
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            }
        }
    }
}
